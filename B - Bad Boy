int main(){
    int t;
    cin>>t;
    while(t--){
        long long int n,m,i,j,x,y,x1,y1;
        cin>>n>>m>>i>>j;
        if(i<n/2)x=n;
        else x=1;
        if(j<m/2)y=m;
        else y=1;
        
        if(x==1)x1=n;
        else x1=1;
        
        if(y==1)y1=m;
        else y1=1;
        
        cout<<x<<" "<<y<<" "<<x1<<" "<<y1<<endl;
    }
}
